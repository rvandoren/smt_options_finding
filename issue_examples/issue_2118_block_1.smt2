(set-logic qf_bv)
(define-fun s1 () (_ bitvec 8) #x07)
(define-fun s3 () (_ bitvec 8) #x06)
(declare-fun s0 () (_ bitvec 8))
(declare-fun s6 () (_ bitvec 8))
(declare-fun s7 () (_ bitvec 8))
